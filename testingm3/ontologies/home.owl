	<!-- ################################################ LuminousEfficacy ############################################################################ -->
	
	<owl:Class rdf:ID="EnergyEfficiencyA">		
		<rdfs:label xml:lang="en">Energy Efficiency A</rdfs:label>
		<rdfs:comment xml:lang="en">Rule: IF m3:LuminousEfficacy greaterThan 210 m3:LumenPerWatt THEN EnergyEfficiencyA</rdfs:comment>
		<dc:description xml:lang="en">Source: https://ec.europa.eu/transparency/regdoc/rep/3/2019/EN/C-2019-1805-F1-EN-ANNEX-1-PART-1.PDF</dc:description>				
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:modified> 
        <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:issued>
	</owl:Class>
	
	<owl:Class rdf:ID="EnergyEfficiencyB">		
		<rdfs:label xml:lang="en">Energy Efficiency B</rdfs:label>
		<rdfs:comment xml:lang="en">Rule: IF m3:LuminousEfficacy greaterThan 185 m3:LumenPerWatt AND lessThan 210 m3:LumenPerWatt THEN EnergyEfficiencyB</rdfs:comment>
		<dc:description xml:lang="en">Source: https://ec.europa.eu/transparency/regdoc/rep/3/2019/EN/C-2019-1805-F1-EN-ANNEX-1-PART-1.PDF</dc:description>				
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:modified> 
        <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:issued>
	</owl:Class>
	
	<owl:Class rdf:ID="EnergyEfficiencyC">		
		<rdfs:label xml:lang="en">Energy Efficiency C</rdfs:label>
		<rdfs:comment xml:lang="en">Rule: IF m3:LuminousEfficacy greaterThan 160 m3:LumenPerWatt AND lessThan 185 m3:LumenPerWatt THEN EnergyEfficiencyC</rdfs:comment>
		<dc:description xml:lang="en">Source: https://ec.europa.eu/transparency/regdoc/rep/3/2019/EN/C-2019-1805-F1-EN-ANNEX-1-PART-1.PDF</dc:description>				
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:modified> 
        <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:issued>
	</owl:Class>
	
	<owl:Class rdf:ID="EnergyEfficiencyD">		
		<rdfs:label xml:lang="en">Energy Efficiency D</rdfs:label>
		<rdfs:comment xml:lang="en">Rule: IF m3:LuminousEfficacy greaterThan 135 m3:LumenPerWatt AND lessThan 160 m3:LumenPerWatt THEN EnergyEfficiencyD</rdfs:comment>
		<dc:description xml:lang="en">Source: https://ec.europa.eu/transparency/regdoc/rep/3/2019/EN/C-2019-1805-F1-EN-ANNEX-1-PART-1.PDF</dc:description>				
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:modified> 
        <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:issued>
	</owl:Class>
	
	<owl:Class rdf:ID="EnergyEfficiencyE">		
		<rdfs:label xml:lang="en">Energy Efficiency E</rdfs:label>
		<rdfs:comment xml:lang="en">Rule: IF m3:LuminousEfficacy greaterThan 110 m3:LumenPerWatt AND lessThan 135 m3:LumenPerWatt THEN EnergyEfficiencyE</rdfs:comment>
		<dc:description xml:lang="en">Source: https://ec.europa.eu/transparency/regdoc/rep/3/2019/EN/C-2019-1805-F1-EN-ANNEX-1-PART-1.PDF</dc:description>				
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:modified> 
        <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:issued>
	</owl:Class>
	
	<owl:Class rdf:ID="EnergyEfficiencyF">		
		<rdfs:label xml:lang="en">Energy Efficiency F</rdfs:label>
		<rdfs:comment xml:lang="en">Rule: IF m3:LuminousEfficacy greaterThan 85 m3:LumenPerWatt AND lessThan 110 m3:LumenPerWatt THEN EnergyEfficiencyF</rdfs:comment>
