  <owl:Class rdf:ID="TV">		
		<rdfs:label xml:lang="en">TV</rdfs:label>
		<rdfs:comment xml:lang="en">An actuator to automatically switch on/off the television. 
Rule: IF m3:Power EQUAL 221 m3:Watt THEN SmartTV</rdfs:comment>
		<rdfs:subClassOf rdf:resource="#Actuator"/>	
		<rdfs:subClassOf rdf:resource="#HomeM2MDevice"/>
		<skos:prefLabel xml:lang="en">TV</skos:prefLabel>
		<skos:altLabel xml:lang="en">Smart TV</skos:altLabel>
		<dc:description xml:lang="en"> Energy Efficient Computing: A Comparison of Raspberry PI with Modern Devices [Shah et al. 2015]</dc:description>		
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:modified> 
        <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:issued> 			
	</owl:Class>
	
					
	<owl:Class rdf:ID="MobilePhone">		
		<rdfs:label xml:lang="en">Mobile Phone</rdfs:label>
		<rdfs:comment xml:lang="en">Mobile Phone can be used to understand emotion or even stress with voice data [Lu et al. 2012] [Chang et al. 2011]
A mobile phone contains various sensors such as accelerometers to deduce activities, etc. E.g., the phone is used to correlate the mood and phone usage. Paper: Moodscope: Building a Mood Sensor from Smartphone Usage Patterns [LiKamWa et al. 2013, ACM International conference on Mobile systems, applications, and services (MobiSys - A-Rank Conference)]</rdfs:comment>
		<rdfs:subClassOf rdf:resource="#M2MDevice"/>
		<rdfs:subClassOf rdf:resource="#HealthM2MDevice"/>
		<rdfs:subClassOf rdf:resource="#EmotionM2MDevice"/>	
		<skos:prefLabel xml:lang="en">Mobile Phone</skos:prefLabel>
		<skos:altLabel xml:lang="en">Android Mobile Phone</skos:altLabel>
		<skos:altLabel xml:lang="en">IPhone Mobile Phone</skos:altLabel>
		<dc:description xml:lang="en"> Energy Efficient Computing: A Comparison of Raspberry PI with Modern Devices [Shah et al. 2015]</dc:description>		
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:modified>
		<dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2018-03-26</dcterms:issued> 	
		<txn:hasImage rdf:resource="http://sensormeasurement.appspot.com/images/sensor/mobilePhone.png"/>
	</owl:Class>
	
	<owl:Class rdf:ID="ScreenSensor">		
		<rdfs:label xml:lang="en">Screen Sensor, Touch Screen Sensor, Screen Monitor, Computer Monitor, PC Monitor</rdfs:label>
		<rdfs:comment xml:lang="en">Screen Sensor can be embedded within robots (e.g., ASTRO robot)</rdfs:comment>
		<dc:description xml:lang="en">Paper: Improving domiciliary robotic services by integrating the ASTRO Robot in an AmIInfrastructure [Cavallo et al. 2017]
</dc:description>
<skos:prefLabel xml:lang="en">Screen</skos:prefLabel>
		<skos:altLabel xml:lang="en">Screen Monitor</skos:altLabel>
		<skos:altLabel xml:lang="en">Computer Monitor</skos:altLabel>
		<skos:altLabel xml:lang="en">Touch Screen Sensor</skos:altLabel>
		<rdfs:subClassOf rdf:resource="#M2MDevice"/>	
		<rdfs:subClassOf rdf:resource="#RoboticM2MDevice"/>	
		<rdfs:subClassOf rdf:resource="#HomeM2MDevice"/>	
		<dc:description xml:lang="en">Energy Efficient Computing: A Comparison of Raspberry PI with Modern Devices [Shah et al. 2015]</dc:description>		
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:modified> 
        <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-07-30</dcterms:issued>
        <txn:hasImage rdf:resource="http://linkedopenreasoning.appspot.com/images/sensor/TouchScreenSensor.png"/>	
	</owl:Class>
	
	<owl:Class rdf:ID="Tablet">		
		<rdfs:label xml:lang="en">Tablet</rdfs:label>
		<rdfs:comment xml:lang="en">An actuator to automatically switch on/off the tablet (IPhone, Android). 
Rule: IF m3:Power EQUAL 221 m3:Watt THEN SmartTV</rdfs:comment>
		<rdfs:subClassOf rdf:resource="#Actuator"/>	
		<rdfs:subClassOf rdf:resource="#HomeM2MDevice"/>
		<skos:prefLabel xml:lang="en">TV</skos:prefLabel>
		<skos:altLabel xml:lang="en">Smart TV</skos:altLabel>
		<dc:description xml:lang="en">Energy Efficient Computing: A Comparison of Raspberry PI with Modern Devices [Shah et al. 2015]</dc:description>		
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:modified> 
        <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:issued> 			
	</owl:Class>	
		
	<owl:Class rdf:ID="Computer">		
		<rdfs:label xml:lang="en">Computer</rdfs:label>
		<rdfs:comment xml:lang="en">An actuator to automatically switch on/off the laptop. 
Rule: IF m3:Power EQUAL 50 m3:Watt THEN m3:Laptop</rdfs:comment>
		<rdfs:subClassOf rdf:resource="#Actuator"/>	
		<rdfs:subClassOf rdf:resource="#HomeM2MDevice"/>
		<skos:prefLabel xml:lang="en">Computer</skos:prefLabel>
		<skos:altLabel xml:lang="en">PC</skos:altLabel>
		<skos:altLabel xml:lang="en">Laptop</skos:altLabel>
		<dc:description xml:lang="en"> Energy Efficient Computing: A Comparison of Raspberry PI with Modern Devices [Shah et al. 2015]</dc:description>		
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-09-20</dcterms:modified> 
		<dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2018-03-26</dcterms:issued> 
	</owl:Class>
	
	<owl:Class rdf:ID="RaspberryPI">		
		<rdfs:label xml:lang="en">Raspberry PI (Model B)</rdfs:label>
		<rdfs:comment xml:lang="en">An actuator to automatically switch on/off the Raspberry PI. 
 Rule: IF m3:Power EQUAL 2.25 m3:Watt THEN m3:RaspberryPI</rdfs:comment>
		<rdfs:subClassOf rdf:resource="#Actuator"/>	
		<rdfs:subClassOf rdf:resource="#HomeM2MDevice"/>
		<dc:description xml:lang="en">Energy Efficient Computing: A Comparison of Raspberry PI with Modern Devices [Shah et al. 2015]</dc:description>		
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-09-20</dcterms:modified> 
		<dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-09-20</dcterms:issued> 
	</owl:Class>

	
	<owl:Class rdf:ID="Telephone">		
		<rdfs:label xml:lang="en">Telephone</rdfs:label>
		<rdfs:comment xml:lang="en">An actuator to automatically switch on/off the Telephone.</rdfs:comment>
		<rdfs:subClassOf rdf:resource="#Actuator"/>	
		<rdfs:subClassOf rdf:resource="#HomeM2MDevice"/>
	</owl:Class>
  
   <!-- ################################################ THERMAL CONDUCTIVITY ############################################################################ -->
	
	<owl:Class rdf:ID="ThermalConductivityAir">		
		<rdfs:label xml:lang="en">Thermal Conductivity Air</rdfs:label>
		<rdfs:comment xml:lang="en">Rule: IF m3:ThermalConductivity EQUAL 0.026 m3:WattPerMeterKelvin THEN ThermalConductivityAir</rdfs:comment>
		<dc:description xml:lang="en">Source: https://en.wikipedia.org/wiki/Thermal_conductivity</dc:description>		
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:modified> 
        <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:issued> 			
	</owl:Class>
	
	<owl:Class rdf:ID="ThermalConductivityStyrofoam">		
		<rdfs:label xml:lang="en">Thermal Conductivity Styrofoam</rdfs:label>
		<rdfs:comment xml:lang="en">Rule: IF m3:ThermalConductivity EQUAL 0.033 m3:WattPerMeterKelvin THEN ThermalConductivityStyrofoam</rdfs:comment>
		<dc:description xml:lang="en">Source: https://en.wikipedia.org/wiki/Thermal_conductivity</dc:description>		
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:modified> 
        <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:issued> 			
	</owl:Class>
	
	<owl:Class rdf:ID="ThermalConductivityWater">		
		<rdfs:label xml:lang="en">Thermal Conductivity Water</rdfs:label>
		<rdfs:comment xml:lang="en">Rule: IF m3:ThermalConductivity EQUAL 0.6089 m3:WattPerMeterKelvin THEN ThermalConductivityWater</rdfs:comment>
		<dc:description xml:lang="en">Source: https://en.wikipedia.org/wiki/Thermal_conductivity</dc:description>		
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:modified> 
        <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:issued> 			
	</owl:Class>
	
	<owl:Class rdf:ID="ThermalConductivityConcrete">		
		<rdfs:label xml:lang="en">Thermal Conductivity Concrete</rdfs:label>
		<rdfs:comment xml:lang="en">Rule: IF m3:ThermalConductivity EQUAL 0.92 m3:WattPerMeterKelvin THEN ThermalConductivityConcrete</rdfs:comment>
		<dc:description xml:lang="en">Source: https://en.wikipedia.org/wiki/Thermal_conductivity</dc:description>		
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:modified> 
        <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:issued> 			
	</owl:Class>
	
	<owl:Class rdf:ID="ThermalConductivityCopper">		
		<rdfs:label xml:lang="en">Thermal Conductivity Copper</rdfs:label>
		<rdfs:comment xml:lang="en">Rule: IF m3:ThermalConductivity EQUAL 384.1 m3:WattPerMeterKelvin THEN ThermalConductivityCopper</rdfs:comment>
		<dc:description xml:lang="en">Source: https://en.wikipedia.org/wiki/Thermal_conductivity</dc:description>		
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:modified> 
        <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:issued> 			
	</owl:Class>
  
  <owl:Class rdf:ID="ThermalConductivityNaturalDiamond">		
		<rdfs:label xml:lang="en">Thermal Conductivity Natural Diamond</rdfs:label>
		<rdfs:comment xml:lang="en">Rule: IF m3:ThermalConductivity BETWEEN 895 AND 1350 m3:WattPerMeterKelvin THEN ThermalConductivityNaturalDiamond</rdfs:comment>
		<dc:description xml:lang="en">Source: https://en.wikipedia.org/wiki/Thermal_conductivity</dc:description>		
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:modified> 
        <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:issued> 			
	</owl:Class>
   <!-- ################################################ ELECTRICAL RESITAANCE OHM ############################################################################ -->
	
	<owl:Class rdf:ID="ResistanceTypeDrySkin">		
		<rdfs:label xml:lang="en">Resistance Type Dry Skin</rdfs:label>
		<rdfs:comment xml:lang="en">Rule: IF m3:ElectricalResistance BETWEEN 100000 AND 600000 m3:Ohm THEN ResistanceTypeDrySkin</rdfs:comment>
		<dc:description xml:lang="en">Source: https://homepages.wmich.edu/~bazuinb/SolarGarden/Danger-Electricity.pdf</dc:description>		
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:modified> 
        <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:issued> 			
	</owl:Class>
	
	<owl:Class rdf:ID="ResistanceTypeWetSkin">		
		<rdfs:label xml:lang="en">Resistance Type Wet Skin</rdfs:label>
		<rdfs:comment xml:lang="en">Rule: IF m3:ElectricalResistance EQUAL 1000 m3:Ohm THEN ResistanceTypeWetSkin</rdfs:comment>
		<dc:description xml:lang="en">Source: https://homepages.wmich.edu/~bazuinb/SolarGarden/Danger-Electricity.pdf</dc:description>		
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:modified> 
        <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:issued> 			
	</owl:Class>
	
	<owl:Class rdf:ID="ResistanceTypeHandToFoot">		
		<rdfs:label xml:lang="en">Resistance Type Hand To Foot</rdfs:label>
		<rdfs:comment xml:lang="en">Rule: IF m3:ElectricalResistance BETWEEN 400 AND 600 m3:Ohm THEN ResistanceTypeHandToFoot</rdfs:comment>
		<dc:description xml:lang="en">Source: https://homepages.wmich.edu/~bazuinb/SolarGarden/Danger-Electricity.pdf</dc:description>		
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:modified> 
        <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:issued> 			
	</owl:Class>
	
	<owl:Class rdf:ID="ResistanceTypeEarToEar">		
		<rdfs:label xml:lang="en">ResistanceType Ear To Ear</rdfs:label>
		<rdfs:comment xml:lang="en">Rule: IF m3:ElectricalResistance EQUAL 100 m3:Ohm THEN ResistanceTypeEarToEar</rdfs:comment>
		<dc:description xml:lang="en">Source: https://homepages.wmich.edu/~bazuinb/SolarGarden/Danger-Electricity.pdf</dc:description>		
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:modified> 
        <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:issued> 			
	</owl:Class>
  
   <!-- ################################################ ELECTRICAL POTENTIAL ############################################################################ -->
	
	<owl:Class rdf:ID="ElectricalPotential_DC_IEC60038_HighVoltageSupplySystem">		
		<rdfs:label xml:lang="en">Electrical Potential DC IEC60038 High Voltage Supply System</rdfs:label>
		<rdfs:comment xml:lang="en">Rule: IF m3:ElectricalPotential (DC) GREATER THAN 1500 m3:Volt THEN IEC60038 Voltage range = HighVoltageSupplySystem AND Defining risk = Electrical Arcing</rdfs:comment>
		<dc:description xml:lang="en">Source: Source: Bachelor Thesis: High voltage components in commercial vehicles [Matti 2017]
Table Voltage levels according to IEC 60038 (IEC 60038, 2002)</dc:description>	
		<m3:hasRecommendation rdf:resource="&m3;ElectricalArcing"/> 	
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:modified> 
        <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:issued> 			
	</owl:Class>
	
	<owl:Class rdf:ID="ElectricalArcing">		
		<rdfs:label xml:lang="en">Electrical Arcing</rdfs:label>
		<rdfs:comment xml:lang="en">Rule: IF m3:ElectricalPotential (DC) GREATER THAN 1500 m3:Volt THEN IEC60038 Voltage range = HighVoltageSupplySystem AND Defining risk = Electrical Arcing</rdfs:comment>
		<dc:description xml:lang="en">Source: Source: Bachelor Thesis: High voltage components in commercial vehicles [Matti 2017]
Table Voltage levels according to IEC 60038 (IEC 60038, 2002)</dc:description>	
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:modified> 
        <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:issued> 			
	</owl:Class>	
	
	<owl:Class rdf:ID="ElectricalPotential_DC_IEC60038_LowVoltageSupplySystem">		
		<rdfs:label xml:lang="en">Electrical Potential DC IEC60038 Low Voltage Supply System</rdfs:label>
		<rdfs:comment xml:lang="en">Rule: IF m3:ElectricalPotential (DC) BETWEEN 120 AND 1500 m3:Volt THEN IEC60038 Voltage range = LowVoltageSupplySystem AND Defining risk = Electrical shock</rdfs:comment>
		<dc:description xml:lang="en">Source: Source: Bachelor Thesis: High voltage components in commercial vehicles [Matti 2017]
Table Voltage levels according to IEC 60038 (IEC 60038, 2002)</dc:description>	
		<m3:hasRecommendation rdf:resource="&m3;ElectricalShock"/> 	
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:modified> 
        <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:issued> 			
	</owl:Class>
	
	<owl:Class rdf:ID="ElectricalShock">		
		<rdfs:label xml:lang="en">Electrical shock</rdfs:label>
		<rdfs:comment xml:lang="en">Rule: IF m3:ElectricalPotential (DC) BETWEEN 120 AND 1500 m3:Volt THEN IEC60038 Voltage range = LowVoltageSupplySystem AND Defining risk = Electrical shock</rdfs:comment>
		<dc:description xml:lang="en">Source: Source: Bachelor Thesis: High voltage components in commercial vehicles [Matti 2017]
Table Voltage levels according to IEC 60038 (IEC 60038, 2002)</dc:description>	
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:modified> 
        <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:issued> 			
	</owl:Class>
	
		<owl:Class rdf:ID="ElectricalPotential_DC_IEC60038_ExtraLowVoltageSupplySystem">		
		<rdfs:label xml:lang="en">Electrical Potential DC IEC60038 Extra Low Voltage Supply System</rdfs:label>
		<rdfs:comment xml:lang="en">Rule: IF m3:ElectricalPotential (DC) LESS THAN 120 m3:Volt THEN IEC60038 Voltage range = ExtraLowVoltageSupplySystem AND Defining risk = LowRisk</rdfs:comment>
		<dc:description xml:lang="en">Source: Source: Bachelor Thesis: High voltage components in commercial vehicles [Matti 2017]
Table Voltage levels according to IEC 60038 (IEC 60038, 2002)</dc:description>	
		<m3:hasRecommendation rdf:resource="&m3;LowRisk"/> 	
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:modified> 
        <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:issued> 			
	</owl:Class>
	
	<owl:Class rdf:ID="LowRisk">		
		<rdfs:label xml:lang="en">Low Risk</rdfs:label>
		<rdfs:comment xml:lang="en">Rule: IF m3:ElectricalPotential (DC) LESS THAN 120 m3:Volt THEN IEC60038 Voltage range = ExtraLowVoltageSupplySystem AND Defining risk = LowRisk</rdfs:comment>
		<dc:description xml:lang="en">Source: Source: Bachelor Thesis: High voltage components in commercial vehicles [Matti 2017]
Table Voltage levels according to IEC 60038 (IEC 60038, 2002)</dc:description>	
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:modified> 
        <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:issued> 			
	</owl:Class>
   
   <!-- ################################################ ELECTRICAL CURRENT ############################################################################ -->
	
	<owl:Class rdf:ID="ElectricCurrentTingling">		
		<rdfs:label xml:lang="en">Electric Current Tingling</rdfs:label>
		<rdfs:comment xml:lang="en">Rule: IF m3:ElectricCurrent EQUAL 0.0011 m3:Ampere THEN HumanReaction at 60Hz = Perception - A slight tingling sensation</rdfs:comment>
		<dc:description xml:lang="en">Source: http://electronicstechnician.tpub.com/14086/css/Amount-Of-Body-Resistance-34.htm</dc:description>	
		<m3:hasRecommendation rdf:resource="&m3;Alert"/> 	
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:modified> 
        <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:issued> 			
	</owl:Class>
	
	<owl:Class rdf:ID="ElectricCurrentCannotLetGoMuscle">		
		<rdfs:label xml:lang="en">Electric Current Cannot Let Go Muscle</rdfs:label>
		<rdfs:comment xml:lang="en">Rule: IF m3:ElectricCurrent EQUAL 0.010 m3:Ampere THEN HumanReaction at 60Hz = Can't let go - Arm and hand muscles close involuntary 120 lb. person</rdfs:comment>
		<dc:description xml:lang="en">Source: http://electronicstechnician.tpub.com/14086/css/Amount-Of-Body-Resistance-34.htm</dc:description>	
		<m3:hasRecommendation rdf:resource="&m3;Alert"/> 	
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:modified> 
        <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:issued> 			
	</owl:Class>
	
	<owl:Class rdf:ID="ElectricCurrentCannotLetGo">		
		<rdfs:label xml:lang="en">Electric Current Cannot Let Go</rdfs:label>
		<rdfs:comment xml:lang="en">Rule: IF m3:ElectricCurrent EQUAL 0.016 m3:Ampere THEN HumanReaction at 60Hz = Can't let go - 175 lb. person</rdfs:comment>
		<dc:description xml:lang="en">Source: http://electronicstechnician.tpub.com/14086/css/Amount-Of-Body-Resistance-34.htm</dc:description>	
		<m3:hasRecommendation rdf:resource="&m3;Alert"/> 	
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:modified> 
        <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:issued> 			
	</owl:Class>
	
	<owl:Class rdf:ID="ElectricCurrentCannotBreathe">		
		<rdfs:label xml:lang="en">Electric Current Cannot Breathe</rdfs:label>
		<rdfs:comment xml:lang="en">Rule: IF m3:ElectricCurrent EQUAL 0.018 m3:Ampere THEN HumanReaction at 60Hz = Can't breathe - Paralysis of the chest muscles</rdfs:comment>
		<dc:description xml:lang="en">Source: http://electronicstechnician.tpub.com/14086/css/Amount-Of-Body-Resistance-34.htm</dc:description>	
		<m3:hasRecommendation rdf:resource="&m3;Alert"/> 	
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:modified> 
        <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:issued> 			
	</owl:Class>
	
	<owl:Class rdf:ID="ElectricCurrentHeartFibrillation">		
		<rdfs:label xml:lang="en">Electric Current Heart Fibrillation</rdfs:label>
		<rdfs:comment xml:lang="en">Rule: IF m3:ElectricCurrent EQUAL 0.065 m3:Ampere THEN HumanReaction at 60Hz = Heart Fibrillation - Rapid irregular contractions of the heart muscles - could be fatal</rdfs:comment>
		<dc:description xml:lang="en">Source: http://electronicstechnician.tpub.com/14086/css/Amount-Of-Body-Resistance-34.htm</dc:description>	
		<m3:hasRecommendation rdf:resource="&m3;Alert"/> 	
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:modified> 
        <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:issued> 			
	</owl:Class>
  
  <!-- ################################################ ROOM TEMPERATURE ############################################################################ -->
	
	<owl:Class rdf:ID="BelowOrZeroTemperature">		
		<rdfs:label xml:lang="en">Below Or Zero Temperature</rdfs:label>
		<rdfs:comment xml:lang="en">Rule: IF m3:Temperature lessThan 0 m3:DegreeCelsius THEN BelowOrZeroTemperature</rdfs:comment>
		<dc:description xml:lang="en">ThinkHome Energy Efficiency in Future Smart Homes [Kofler 2011]</dc:description>	
		<m3:hasRecommendation rdf:resource="&m3;Alert"/> 	
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:modified> 
        <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:issued> 			
	</owl:Class>
	
	<owl:Class rdf:ID="LowTemperature">		
		<rdfs:label xml:lang="en">Low Temperature</rdfs:label>
		<rdfs:comment xml:lang="en">Rule: IF m3:Temperature lessThan 25 m3:DegreeCelsius THEN LowTemperature. Room Temperature is considered as LOW when less than 25 Degree Celcius. This rule has been designed for the WWW Demo Paper 2017</rdfs:comment>
		<dc:description xml:lang="en">SWoTSuite: A Toolkit for Prototyping End-to-End Semantic Web of Things Applications [Pankesh et al. WWW Demo Paper 2017]</dc:description>	
		<m3:hasRecommendation rdf:resource="&m3;Alert"/> 	
		<m3:hasRecommendation rdf:resource="&home;SwitchOffHeating"/>
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:modified> 
        <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:issued> 			
	</owl:Class>
	
	<owl:Class rdf:ID="Frost">		
		<rdfs:label xml:lang="en">Frost</rdfs:label>
		<rdfs:comment xml:lang="en">IF m3:Temperature greaterThan -25 m3:DegreeCelsius AND lessThan 0 m3:DegreeCelsius THEN Frost</rdfs:comment>
		<dc:description xml:lang="en">ThinkHome Energy Efficiency in Future Smart Homes [Kofler 2011]</dc:description>	
		<m3:hasRecommendation rdf:resource="&m3;Alert"/> 	
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:modified> 
        <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:issued> 			
	</owl:Class>
	
	<owl:Class rdf:ID="ExtremeFrost">		
		<rdfs:label xml:lang="en">Extreme Frost</rdfs:label>
		<rdfs:comment xml:lang="en">IF m3:Temperature lessThan -25 m3:DegreeCelsius THEN ExtremeFrost</rdfs:comment>
		<dc:description xml:lang="en">ThinkHome Energy Efficiency in Future Smart Homes [Kofler 2011]</dc:description>	
		<m3:hasRecommendation rdf:resource="&m3;Alert"/> 
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:modified> 
        <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:issued> 				
	</owl:Class>
	
	<owl:Class rdf:ID="ExtremeHeat">		
		<rdfs:label xml:lang="en">Extreme Heat</rdfs:label>
		<rdfs:comment xml:lang="en">IF m3:Temperature greaterThan 37 m3:DegreeCelsius THEN ExtremeHeat</rdfs:comment>
		<dc:description xml:lang="en">ThinkHome Energy Efficiency in Future Smart Homes [Kofler 2011]</dc:description>	
		<m3:hasRecommendation rdf:resource="&m3;Alert"/> 	
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:modified> 
        <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:issued> 		
	</owl:Class>
	
	<owl:Class rdf:ID="AboveRoomTemperature">		
		<rdfs:label xml:lang="en">Above Room Temperature</rdfs:label>
		<rdfs:comment xml:lang="en">IF m3:Temperature greaterThan 25 AND lessThan 30  m3:DegreeCelsius THEN AboveRoomTemperature</rdfs:comment>
		<dc:description xml:lang="en">ThinkHome Energy Efficiency in Future Smart Homes [Kofler 2011]</dc:description>					
		<m3:hasRecommendation rdf:resource="&home;SwitchOffHeating"/> 
		<m3:hasRecommendation rdf:resource="&home;SwitchOnAirConditioning"/> 
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:modified> 
        <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:issued> 
	</owl:Class>
	
	<owl:Class rdf:ID="BelowRoomTemperature">		
		<rdfs:label xml:lang="en">Below Room Temperature</rdfs:label>
		<rdfs:comment xml:lang="en">IF m3:Temperature greaterThan 10 AND lessThan 20 m3:DegreeCelsius THEN BelowRoomTemperature</rdfs:comment>
		<dc:description xml:lang="en">ThinkHome Energy Efficiency in Future Smart Homes [Kofler 2011]</dc:description>		
		<m3:hasRecommendation rdf:resource="&home;SwitchOnHeating"/> 
		<m3:hasRecommendation rdf:resource="&home;SwitchOffAirConditioning"/> 		
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:modified> 
        <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:issued> 
	</owl:Class>
	
	<owl:Class rdf:ID="RoomTemperature">		
		<rdfs:label xml:lang="en">Room Temperature</rdfs:label>
		<rdfs:comment xml:lang="en">IF m3:Temperature greaterThan 20 m3:DegreeCelsius AND lessThan 25 m3:DegreeCelsius THEN RoomTemperature</rdfs:comment>
		<dc:description xml:lang="en">ThinkHome Energy Efficiency in Future Smart Homes [Kofler 2011]</dc:description>		
		<m3:hasRecommendation rdf:resource="&m3;NothingAnormal"/> 
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:modified> 
        <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:issued> 			
	</owl:Class>
	
	<owl:Class rdf:ID="AboveZeroTemperature">		
		<rdfs:label xml:lang="en">Above Zero Temperature</rdfs:label>
		<rdfs:comment xml:lang="en">IF m3:Temperature greaterThan 0 m3:DegreeCelsius THEN AboveZeroTemperature</rdfs:comment>
		<dc:description xml:lang="en">ThinkHome Energy Efficiency in Future Smart Homes [Kofler 2011]</dc:description>		
		<m3:hasRecommendation rdf:resource="&m3;Alert"/> 
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:modified> 
        <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:issued> 			
	</owl:Class>
	
	<owl:Class rdf:ID="Heat">		
		<rdfs:label xml:lang="en">Heat</rdfs:label>
		<rdfs:comment xml:lang="en">IF m3:Temperature greaterThan 30 m3:DegreeCelsius THEN Heat</rdfs:comment>
		<dc:description xml:lang="en">ThinkHome Energy Efficiency in Future Smart Homes [Kofler 2011]</dc:description>	
		<m3:hasRecommendation rdf:resource="&home;SwitchOnAirConditioning"/>
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:modified> 
        <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-25</dcterms:issued> 		
	</owl:Class>
  
